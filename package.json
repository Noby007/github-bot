{
  "name": "blitz-bot",
  "version": "1.0.0",
  "private": true,
  "description": "automate project board",
  "author": "dajinchu <dajinchu@gmail.com>",
  "license": "ISC",
  "repository": "https://github.com/blitz-js/bot-github",
  "homepage": "https://github.com/blitz-js/bot-github",
  "bugs": "https://github.com/blitz-js/bot-github/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "yarn build:watch",
    "start": "probot run ./lib/index.js",
    "test": "jest",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "all-contributors-cli": "^6.20.0",
    "lodash": "^4.17.21",
    "probot": "^11.0.6"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.31",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "4.3.8",
    "jest": "^26.6.3",
    "nock": "^13.0.9",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "smee-client": "^1.2.2",
    "ts-jest": "^26.5.2",
    "typescript": "^4.2.2"
  },
  "engines": {
    "node": "14.x"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
